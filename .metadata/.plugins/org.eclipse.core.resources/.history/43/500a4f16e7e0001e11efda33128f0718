package com.krakedev;

public class Rectangulo {
	
	public int base;
	public int altura;
	
	public int CalcularArea() {
		int area;
		area= base * altura;
		return area ;
		
	}
	
	public int CalcularPerimetro() {
		int perimetro;
		perimetro= (base+ base)+(altura+altura);
		return perimetro;
		
	}
	
	

}
